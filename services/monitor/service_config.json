{
  "service": {
    "name": "dadm-monitor",
    "type": "monitor",
    "version": "1.0.0",
    "description": "DADM Stack Monitor Service",
    "port": 5200,
    "health_endpoint": "/health",
    "metadata": {
      "monitors": ["dadm-openai-assistant", "dadm-echo-service", "camunda", "consul"],
      "check_interval": 60,
      "restart_failed": true,
      "log_level": "INFO"
    }
  }
}

# Analysis Service Environment Variables
# Copy this file to .env or set these variables in your environment

# Service Configuration
ANALYSIS_SERVICE_PORT=8002
ANALYSIS_SERVICE_NAME=dadm-analysis-service

# Integration URLs - CUSTOMIZE THESE FOR YOUR ENVIRONMENT
ANALYSIS_PROMPT_SERVICE_URL=http://localhost:5300
ANALYSIS_CAMUNDA_BASE_URL=http://dadm-camunda:8080

# LLM Configuration
ANALYSIS_LLM_PROVIDER=openai
ANALYSIS_LLM_MODEL=gpt-4
ANALYSIS_LLM_TEMPERATURE=0.3
ANALYSIS_MAX_TOKENS=4000

# Service Discovery (optional)
ANALYSIS_CONSUL_ENABLED=false
ANALYSIS_CONSUL_URL=http://consul:8500

# Validation Settings
ANALYSIS_STRICT_VALIDATION=true
ANALYSIS_MAX_RETRY_ATTEMPTS=3
ANALYSIS_VALIDATION_TIMEOUT=30.0

# Storage and Caching
ANALYSIS_STORAGE_PATH=./analysis_results
ANALYSIS_CACHE_RESULTS=true
ANALYSIS_CACHE_TTL=3600

# BPMN Integration
ANALYSIS_WORKFLOW_INTEGRATION=true

# Configuration File Path (optional)
ANALYSIS_CONFIG_FILE=./config.json

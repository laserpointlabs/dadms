{
  "service": {
    "name": "dadm-analysis-service",
    "type": "analysis",
    "port": 8002,
    "version": "0.10.0",
    "health_endpoint": "/health",
    "description": "DADM Analysis Service for structured LLM-driven analysis with BPMN workflow integration",
    "tags": [
      "analysis",
      "llm",
      "bpmn",
      "decision-support",
      "workflow"
    ],
    "meta": {
      "supported_templates": "business_analysis,decision_analysis,risk_analysis",
      "integrates_with": "prompt-service,camunda",
      "llm_providers": "openai,anthropic",
      "output_formats": "json,structured_text,markdown"
    },
    "dependencies": {
      "prompt_service": {
        "name": "dadm-prompt-service",
        "required": true,
        "health_check": true
      },
      "consul": {
        "name": "consul",
        "required": true,
        "service_discovery": true
      }
    }
  }
}

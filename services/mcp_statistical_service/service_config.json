{  "service": {
    "name": "mcp-statistical-service",
    "type": "mcp",
    "version": "1.0",
    "port": 5201,
    "description": "Statistical analysis service powered by MCP servers",
    "health_endpoint": "/health",
    "tags": [
      "mcp",
      "analytics",
      "statistics"
    ],
    "metadata": {
      "api_version": "v1",
      "mcp_version": "1.0",
      "service_category": "analytics"
    },
    "health_check": {
      "interval": "10s",
      "timeout": "5s",
      "retries": 3
    },
    "consul": {
      "register": true,
      "deregister_on_shutdown": true
    },
    "capabilities": [
      "descriptive_statistics",
      "hypothesis_testing", 
      "regression_analysis",
      "time_series_analysis",
      "statistical_modeling"
    ],
    "mcp_config": {
      "server_executable": "python",
      "server_args": ["-m", "mcp_statistical_server"],
      "supported_tools": [
        "calculate_statistics",
        "run_statistical_test", 
        "fit_regression_model",
        "analyze_time_series"
      ]
    }
  },
  "docker": {
    "image": "dadm/mcp-statistical-service",
    "ports": ["5201:5201"],
    "environment": {
      "MCP_SERVER_PATH": "/app/mcp_servers",
      "PYTHON_PATH": "/app:/app/mcp_servers"
    }
  }
}

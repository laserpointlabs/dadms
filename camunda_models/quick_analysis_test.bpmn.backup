<?xml version='1.0' encoding='utf-8'?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:ns2="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:ns3="http://www.omg.org/spec/DD/20100524/DC" xmlns:ns4="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:process id="QuickAnalysisTest" name="Quick Analysis Test" isExecutable="true" camunda:historyTimeToLive="30">
    
    
    <bpmn:startEvent id="StartEvent_1" name="Start Quick Test" camunda:formKey="embedded:deployment:quick-test-form.html">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="test_data" label="Test Data (numbers)" type="string" defaultValue="1,2,3,4,5">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="analysis_description" label="What to analyze" type="string" defaultValue="Find the mean and trend of the data" />
        </camunda:formData>
      </bpmn:extensionElements>
    </bpmn:startEvent>
    
    
    <bpmn:serviceTask id="ServiceTask_CallAnalysis" name="Call Analysis Service" camunda:type="external" camunda:topic="service_task_call_analysis_task">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://analysis-service:8002/analyze/integrated</camunda:inputParameter>
            <camunda:inputParameter name="method">POST</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:inputParameter name="payload">
              <camunda:script scriptFormat="groovy">
                
                import groovy.json.JsonBuilder

                // Parse the input data
                def dataArray = test_data.split(',').collect { it.trim() as Double }
                
                def request = [
                  analysis_type: "quick_test",
                  data_sources: [
                    input_data: dataArray,
                    description: analysis_description
                  ],
                  execution_tools: ["python"],
                  analysis_parameters: [
                    calculate_mean: true,
                    calculate_trend: true,
                    generate_summary: true
                  ],
                  timeout: 120,
                  metadata: [
                    workflow_id: execution.getProcessInstanceId(),
                    task_id: "ServiceTask_CallAnalysis",
                    timestamp: new Date().toString()
                  ]
                ]
                
                return new JsonBuilder(request).toString()
                
              </camunda:script>
            </camunda:inputParameter>
            <camunda:outputParameter name="analysis_execution_id">${response.execution_id}</camunda:outputParameter>
            <camunda:outputParameter name="analysis_status">${response.status}</camunda:outputParameter>
            <camunda:outputParameter name="analysis_results">${response}</camunda:outputParameter>
          </camunda:inputOutput>
        </camunda:connector>
      </bpmn:extensionElements>
    </bpmn:serviceTask>
    
    
    <bpmn:userTask id="UserTask_ShowResults" name="Review Analysis Results" camunda:formKey="embedded:deployment:results-display.html">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="analysis_execution_id" label="Execution ID" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="analysis_status" label="Status" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="analysis_results" label="Full Results (JSON)" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="test_complete" label="Test Completed Successfully" type="boolean" defaultValue="true" />
        </camunda:formData>
      </bpmn:extensionElements>
    </bpmn:userTask>
    
    
    <bpmn:endEvent id="EndEvent_1" name="Test Complete">
      <bpmn:incoming>Flow_3</bpmn:incoming>
    </bpmn:endEvent>
    
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="ServiceTask_CallAnalysis" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="ServiceTask_CallAnalysis" targetRef="UserTask_ShowResults" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="UserTask_ShowResults" targetRef="EndEvent_1" />
    
  </bpmn:process>
  
  
  <ns2:BPMNDiagram id="BPMNDiagram_1">
    <ns2:BPMNPlane id="BPMNPlane_1" bpmnElement="QuickAnalysisTest">
      
      
      <ns2:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <ns3:Bounds x="179" y="99" width="36" height="36" />
        <ns2:BPMNLabel>
          <ns3:Bounds x="159" y="142" width="76" height="14" />
        </ns2:BPMNLabel>
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="ServiceTask_CallAnalysis_di" bpmnElement="ServiceTask_CallAnalysis">
        <ns3:Bounds x="270" y="77" width="100" height="80" />
        <ns2:BPMNLabel />
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="UserTask_ShowResults_di" bpmnElement="UserTask_ShowResults">
        <ns3:Bounds x="430" y="77" width="100" height="80" />
        <ns2:BPMNLabel />
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <ns3:Bounds x="592" y="99" width="36" height="36" />
        <ns2:BPMNLabel>
          <ns3:Bounds x="576" y="142" width="68" height="14" />
        </ns2:BPMNLabel>
      </ns2:BPMNShape>
      
      
      <ns2:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <ns4:waypoint x="215" y="117" />
        <ns4:waypoint x="270" y="117" />
      </ns2:BPMNEdge>
      
      <ns2:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <ns4:waypoint x="370" y="117" />
        <ns4:waypoint x="430" y="117" />
      </ns2:BPMNEdge>
      
      <ns2:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <ns4:waypoint x="530" y="117" />
        <ns4:waypoint x="592" y="117" />
      </ns2:BPMNEdge>
      
    </ns2:BPMNPlane>
  </ns2:BPMNDiagram>
</bpmn:definitions>
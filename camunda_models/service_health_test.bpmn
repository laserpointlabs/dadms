<?xml version='1.0' encoding='utf-8'?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:ns2="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:ns3="http://www.omg.org/spec/DD/20100524/DC" xmlns:ns4="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:process id="ServiceHealthTest" name="Service Health Test" isExecutable="true" camunda:historyTimeToLive="30">
    
    
    <bpmn:startEvent id="StartEvent_1" name="Start Health Test">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    
    <bpmn:serviceTask id="ServiceTask_HealthCheck" name="Check Analysis Service Health" camunda:type="external" camunda:topic="service_task_health_check_task">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://analysis-service:8002/health</camunda:inputParameter>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="service_status">${response.status}</camunda:outputParameter>
            <camunda:outputParameter name="service_name">${response.service}</camunda:outputParameter>
            <camunda:outputParameter name="health_response">${response}</camunda:outputParameter>
          </camunda:inputOutput>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    
    
    <bpmn:serviceTask id="ServiceTask_SimpleAnalysis" name="Simple Analysis Call" camunda:type="external" camunda:topic="health-test-analysis">
      <bpmn:documentation>Perform a simple analysis test to verify the analysis service is working correctly. Your task is to:

1. Process a simple test dataset [1, 2, 3, 4, 5]
2. Calculate basic statistics (mean, sum, count)
3. Verify the analysis service is responding properly
4. Return test results for health monitoring

This is a health check task to ensure the analysis service is operational.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:properties>
          <camunda:property name="service.type" value="analysis" />
          <camunda:property name="service.name" value="dadm-analysis-service" />
          <camunda:property name="service.version" value="0.10.0" />
        </camunda:properties>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    
    
    <bpmn:userTask id="UserTask_ShowTestResults" name="Show Test Results">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="service_status" label="Service Health Status" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="service_name" label="Service Name" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="health_response" label="Health Check Response" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="analysis_execution_id" label="Analysis Execution ID" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="analysis_status" label="Analysis Status" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
          <camunda:formField id="analysis_response" label="Analysis Response" type="string">
            <camunda:properties>
              <camunda:property id="readonly" value="true" />
            </camunda:properties>
          </camunda:formField>
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>
    
    
    <bpmn:endEvent id="EndEvent_1" name="Test Complete">
      <bpmn:incoming>Flow_4</bpmn:incoming>
    </bpmn:endEvent>
    
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="ServiceTask_HealthCheck" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="ServiceTask_HealthCheck" targetRef="ServiceTask_SimpleAnalysis" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="ServiceTask_SimpleAnalysis" targetRef="UserTask_ShowTestResults" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="UserTask_ShowTestResults" targetRef="EndEvent_1" />
    
  </bpmn:process>
  
  
  <ns2:BPMNDiagram id="BPMNDiagram_1">
    <ns2:BPMNPlane id="BPMNPlane_1" bpmnElement="ServiceHealthTest">
      
      
      <ns2:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <ns3:Bounds x="152" y="99" width="36" height="36" />
        <ns2:BPMNLabel>
          <ns3:Bounds x="130" y="142" width="80" height="14" />
        </ns2:BPMNLabel>
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="ServiceTask_HealthCheck_di" bpmnElement="ServiceTask_HealthCheck">
        <ns3:Bounds x="240" y="77" width="100" height="80" />
        <ns2:BPMNLabel />
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="ServiceTask_SimpleAnalysis_di" bpmnElement="ServiceTask_SimpleAnalysis">
        <ns3:Bounds x="390" y="77" width="100" height="80" />
        <ns2:BPMNLabel />
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="UserTask_ShowTestResults_di" bpmnElement="UserTask_ShowTestResults">
        <ns3:Bounds x="540" y="77" width="100" height="80" />
        <ns2:BPMNLabel />
      </ns2:BPMNShape>
      
      
      <ns2:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <ns3:Bounds x="692" y="99" width="36" height="36" />
        <ns2:BPMNLabel>
          <ns3:Bounds x="676" y="142" width="68" height="14" />
        </ns2:BPMNLabel>
      </ns2:BPMNShape>
      
      
      <ns2:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <ns4:waypoint x="188" y="117" />
        <ns4:waypoint x="240" y="117" />
      </ns2:BPMNEdge>
      
      <ns2:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <ns4:waypoint x="340" y="117" />
        <ns4:waypoint x="390" y="117" />
      </ns2:BPMNEdge>
      
      <ns2:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <ns4:waypoint x="490" y="117" />
        <ns4:waypoint x="540" y="117" />
      </ns2:BPMNEdge>
      
      <ns2:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <ns4:waypoint x="640" y="117" />
        <ns4:waypoint x="692" y="117" />
      </ns2:BPMNEdge>
      
    </ns2:BPMNPlane>
  </ns2:BPMNDiagram>
</bpmn:definitions>
# DADM React UI Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8001

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_THREAD_CONTEXT=true
REACT_APP_ENABLE_CLI_MANAGER=true

# Development Settings
REACT_APP_LOG_LEVEL=debug
REACT_APP_MOCK_API=false

# UI Configuration
REACT_APP_APP_NAME=DADM - Decision Analysis & Decision Management
REACT_APP_VERSION=1.0.0

# WebSocket Configuration
REACT_APP_WS_RECONNECT_ATTEMPTS=5
REACT_APP_WS_RECONNECT_DELAY=1000

# Authentication (if implemented later)
REACT_APP_AUTH_ENABLED=false
REACT_APP_AUTH_PROVIDER=local

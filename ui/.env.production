# Production Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=https://your-dadm-api.com
REACT_APP_WS_URL=wss://your-dadm-api.com

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_THREAD_CONTEXT=true
REACT_APP_ENABLE_CLI_MANAGER=true

# Production Settings
REACT_APP_LOG_LEVEL=error
REACT_APP_MOCK_API=false

# UI Configuration
REACT_APP_APP_NAME=DADM - Decision Analysis & Decision Management
REACT_APP_VERSION=1.0.0

# WebSocket Configuration
REACT_APP_WS_RECONNECT_ATTEMPTS=3
REACT_APP_WS_RECONNECT_DELAY=2000

# Authentication
REACT_APP_AUTH_ENABLED=true
REACT_APP_AUTH_PROVIDER=oauth
